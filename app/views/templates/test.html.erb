<div class="inside" align="center">

	<div id="calendar" calendar>
		<div class="shell">
			<div class="event" ng:repeat="event in events" style="top:{{event.setTop}}px; left:{{event.setLeft}}px; width:{{event.setWidth}}px;">{{event.title}}</div>
		</div>
		<table class="heart" cellpadding="0" cellspacing="0">
			<tr ng:repeat="week in w">
				<td ng:repeat="day in d" height="{{cellheight}}" width="{{cellwidth}}" valign="top" align="right" class="{{setDate(week,day).type}}date">
					<div>{{setDate(week,day).date}}</div>
				</td>
			</tr>
		</table>
	</div>

</div>