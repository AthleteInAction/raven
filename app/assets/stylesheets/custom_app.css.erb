html,body {

}
.angular {
	width: 100%;
	height: 100%;
}
.angular .inside {
	margin-top:50px;
}
.inside .events {
	float: left;
	width: 200px;
	padding: 3px;
	background: #505050;
	border-radius: 2px;
}
.inside .section-wrap {
	padding: 3px;
	background: #505050;
	border-radius: 2px;
}
ul.menu {
	background: #6f6f6f;
	list-style: none;
	border-radius: 2px;
	border: 1px solid #3f3f3f;
	padding: 0;
	margin-top: 0;
	margin-bottom: 0;
	display: block;
}
ul.menu li.section {
	display: list-item;
}
ul.menu li.section .section-head {
	background-color: #434343;
	background-image: -moz-linear-gradient(#4b4b4b, #434343);
	background-image: -webkit-linear-gradient(#4b4b4b, #434343);
	background-image: linear-gradient(#4b4b4b, #434343);
	background-repeat: repeat-x;
	display: table;
	width: 100%;
	padding: 0;
	
	color: #CCC;
	font-weight: bold;
	font-size: 18px;
	line-height: 20px;
	text-shadow: 0 2px 0 rgba(50,50,50,0.7);
	border-left: 0 none;
}
ul.menu li.section .section-head .td {
	padding:10px !important;
}
ul.menu li.section .section-head .td a {
	font-size: 14px;
	font-weight: normal;
}
ul.menu li.section ul.expanded {
	list-style: none;
	display: block;
	padding: 0;
	margin-top: 0;
	margin-bottom: 0;
}
ul.menu li.section ul.expanded li {
	border-bottom: 1px solid #505050;
	border-top: 1px solid #777;
	font: 13px Helvetica, arial, freesans, clean, sans-serif;
	line-height: 1.4;
}
ul.menu li.section ul.expanded li:first-child {
	border-top: none;
}
ul.menu li.section ul.expanded li:last-child {
	border-bottom: none;
}
ul.menu li.section ul.expanded li a.nav-item,ul.menu li.section ul.expanded li div.blank {
	display: block;
	padding: 8px 10px 8px 8px;
	border-left: 2px solid #777;
	color: #4CBC91;
	text-decoration: none;
	font-size: 12px;
}
ul.menu li.section ul.expanded li a.nav-item:hover {
	background-color: #797979;
}
.small {
	font-size: 11px;
	color:#b4b4b4;
}